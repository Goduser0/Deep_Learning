   "source": [
    "**矩阵转置（transpose）**：沿着对角线反转"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "A:\n",
      "[[1. 2.]\n",
      " [1. 0.]\n",
      " [2. 3.]]\n",
      "A的转置:\n",
      "[[1. 1. 2.]\n",
      " [2. 0. 3.]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([[1.0,2.0],[1.0,0.0],[2.0,3.0]])\n",
    "A_t = np.transpose(A)\n",
    "print(\"A:\" + '\\n' + str(A))\n",
    "print(\"A的转置:\" + '\\n' + str(A_t))\n"
   ]
  }
 ],
